{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": [],
    "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["state"] }]
  },
  "globals": {
    "document": true
  }
}